{
  "name": "angular-hispano",
  "version": "0.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mobile-testing": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:sm": "ng build --configuration=staging --source-map",
    "build:staging": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=production",
    "build:prod:ci": "npm run ngcc && ng build --configuration=production",
    "deploy": "npm run build:staging && firebase use staging && firebase deploy --only hosting",
    "deploy:prod": "npm run build:prod && firebase use production && firebase deploy --only hosting",
    "format": "npm run prettier",
    "ngcc": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,md,ts,html,component.html}\"",
    "lint": "ng lint",
    "smex": "source-map-explorer",
    "stylelint": "stylelint \"**/*.scss\" --config .stylelintrc --syntax scss",
    "test": "ng test",
    "test:ci": "npm run ngcc && ng test --watch=false --browsers='ChromeHeadlessNoSandbox,FirefoxHeadless'",
    "update": "npm-check -u"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "^12.1.0",
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/fire": "^7.6.1",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@angular/service-worker": "^15.2.9",
    "firebase": "^9.23.0",
    "first-input-delay": "^0.1.3",
    "ngx-auth-firebaseui": "^7.1.0",
    "proxy-polyfill": "^0.3.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1502.9",
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-devkit/core": "^15.2.9",
    "@angular-devkit/schematics": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@babel/core": "^7.18.13",
    "@schuchard/prettier": "^5.0.0",
    "@types/jasmine": "^3.7.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "firebase-tools": "^11.25.3",
    "husky": "^4.3.8",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^11.0.0",
    "minimatch": "^3.0.8",
    "npm-check": "^6.0.1",
    "postcss": "^8.2.15",
    "prettier": "~2.6.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^13.13.1",
    "ts-node": "^9.1.1",
    "tsutils": "^3.21.0",
    "typescript": "~4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint && npm run stylelint"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,component.html}": [
      "prettier --write"
    ]
  }
}