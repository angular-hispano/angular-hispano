{
  "name": "angular-hispano",
  "version": "0.3.0",
  "scripts": {
    "e2e": "ng e2e",
    "ng": "ng",
    "start": "ng serve",
    "start:mobile-testing": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:sm": "ng build --prod --source-map",
    "build:prod": "ng build --prod",
    "deploy": "ng deploy",
    "format": "npm run prettier",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,md,ts,html,component.html}\"",
    "lint": "ng lint",
    "smex": "source-map-explorer",
    "stylelint": "stylelint '**/*.scss' --config .stylelintrc --syntax scss",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers='ChromeHeadlessNoSandbox,FirefoxHeadless'",
    "update": "npm-check -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.9",
    "@angular/cdk": "~9.0.0-next.0",
    "@angular/common": "~9.0.0-next.9",
    "@angular/compiler": "~9.0.0-next.9",
    "@angular/core": "~9.0.0-next.9",
    "@angular/fire": "5.2.1",
    "@angular/forms": "~9.0.0-next.9",
    "@angular/material": "^9.0.0-next.0",
    "@angular/platform-browser": "~9.0.0-next.9",
    "@angular/platform-browser-dynamic": "~9.0.0-next.9",
    "@angular/router": "~9.0.0-next.9",
    "@angular/service-worker": "~9.0.0-next.9",
    "angulartics2": "^8.1.0",
    "core-js": "^2.6.9",
    "firebase": "^6.4.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.0-next.8",
    "@angular-devkit/build-angular": "~0.900.0-next.8",
    "@angular-devkit/core": "^9.0.0-next.8",
    "@angular-devkit/schematics": "^9.0.0-next.8",
    "@angular/cli": "~9.0.0-next.8",
    "@angular/compiler-cli": "~9.0.0-next.9",
    "@angular/language-service": "~9.0.0-next.9",
    "@schuchard/prettier": "^3.1.0",
    "@types/jasmine": "^3.4.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.7.11",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^7.5.0",
    "husky": "3.0.9",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "9.4.2",
    "minimatch": "^3.0.4",
    "npm-check": "^5.9.0",
    "prettier": "1.19.1",
    "protractor": "^6.0.0",
    "source-map-explorer": "^2.1.0",
    "stylelint": "^11.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint && npm run stylelint"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
