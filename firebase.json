{
  "hosting": [
    {
      "target": "web",
      "public": "dist/web/browser",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "appAssociation": "AUTO",
      "rewrites": [
        {
          "source": "/enlaces/**",
          "dynamicLinks": true
        },
        {
          "source": "/**/!(*.@(js|ts|html|css|json|svg|png|jpg|jpeg|webp|gif))",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
